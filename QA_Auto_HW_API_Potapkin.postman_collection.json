		{
			"name": "Edits user's profile",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response matches request data\", function () {",
							"    const request = JSON.parse(pm.request.body.raw); ",
							"    const response = pm.response.json().data;         ",
							"",
							"    pm.expect(response.name).to.eql(request.name);",
							"    pm.expect(response.lastName).to.eql(request.lastName);",
							"    pm.expect(response.country).to.eql(request.country);",
							"    ",
							"    function compareDates(date1, date2) {",
							"        const d1 = new Date(date1);",
							"        const d2 = new Date(date2);",
							"",
							"        return d1.getFullYear() === d2.getFullYear() &&",
							"               d1.getMonth() === d2.getMonth() &&",
							"               d1.getDate() === d2.getDate();",
							"    }",
							"",
							"    const requestDate = request.dateBirth;",
							"    const responseDate = response.dateBirth;",
							"    pm.expect(compareDates(requestDate, responseDate)).to.be.true;",
							"});",
							"",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const name = pm.variables.replaceIn ('{{$randomFirstName}}');\r",
							"pm.collectionVariables.set ('name', name);\r",
							"\r",
							"const lastName = pm.variables.replaceIn ('{{$randomLastName}}');\r",
							"pm.collectionVariables.set ('lastName', lastName);\r",
							"\r",
							"const country = pm.variables.replaceIn ('{{$randomCountry}}');\r",
							"pm.collectionVariables.set ('country', country);\r",
							"\r",
							"const timestamp = Date.now();\r",
							"const photo = `user-${timestamp}.jpg`;\r",
							"pm.collectionVariables.set('photo', photo);\r",
							"\r",
							"function randomDate(startYear = 1940, endYear = 2005) {\r",
							"    const start = new Date(`${startYear}-01-01`).getTime();\r",
							"    const end = new Date(`${endYear}-12-31`).getTime();\r",
							"    const random = new Date(start + Math.random() * (end - start));\r",
							"\r",
							"    random.setMilliseconds(0);\r",
							"\r",
							"    return random.toISOString();\r",
							"}\r",
							"\r",
							"const dateBirth = randomDate();\r",
							"pm.collectionVariables.set('dateBirth', dateBirth);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "PUT",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"photo\": \"{{photo}}\",\r\n  \"name\": \"{{name}}\",\r\n  \"lastName\": \"{{lastName}}\",\r\n  \"dateBirth\": \"{{dateBirth}}\",\r\n  \"country\": \"{{country}}\"\r\n}",
					"options": {
